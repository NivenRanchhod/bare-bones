{
  "name": "eleventy-blog",
  "version": "1.1.0",
  "description": "An example blog site using Eleventy that covers fundamental functionality",
  "scripts": {
    "watch:sass": "npx sass sass:_site/css --watch",
    "build:sass": "npx sass sass:_site/css",
    "watch:eleventy": "npx @11ty/eleventy --serve",
    "start": "npm-run-all build:sass --parallel watch:*",
    "build": "npm-run-all build:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JonUK/eleventy-blog.git"
  },
  "author": "Jon Keeping",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JonUK/eleventy-blog/issues"
  },
  "homepage": "https://github.com/JonUK/eleventy-blog#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-navigation": "^0.3.4",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.1.0",
    "jest": "^26.6.3",
    "moment": "^2.29.4"
  },
  "dependencies": {
    "@jest/globals": "latest",
    "npm": "^8.14.0",
    "run-all": "^1.0.1",
    "sass": "^1.53.0",
    "save-dev": "0.0.1-security"
  }
}
